{
    "manifest_version": 2,
    "name": "kufu",
    "description": "Tracks the websites you visit and the time you spend on each one.",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "kufu"
    },
    "permissions": [
        "tabs",
        "alarms",
        "storage"
    ]
}
