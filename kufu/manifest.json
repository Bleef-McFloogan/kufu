{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "Tracks the websites you visit and the time you spend on each one",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
	"default_title": "Kufu"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
